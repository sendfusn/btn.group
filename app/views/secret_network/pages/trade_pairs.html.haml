- content_for :header_banner do
  .header-banner#secret-network-trade-pairs
    .nk-ovm.shape-q
%main.nk-pages
  %section.bg-light.pt-4
    .container-fluid
      .nk-block.nk-block-blog
        .row
          .col-12
            .card.my-3
              %h3 Trade pairs
              %p.mb-0
                %b Blockchain:
                = cl_image_tag(features[:secret_network][:cloudinary_public_id], width: 18, height: 18, crop: :fill, dpr: 2, aspect_ratio: 1, fetch_format: :auto, quality: :auto)
                = features[:secret_network][:name]
              .table-responsive
                %table.table.table-striped#trade-pairs-table
