.header-banner.bg-theme-grad-s2
  .nk-banner
    .banner.banner-page
      .mt-3.w-100
        .container#secret-network-yield-optimizer
          .row.mb-3
            .col-12
              .alert.alert-warning-alt
                Warning! These pools are not private.
          .row.mb-3
            .col-12
              .card
                .card-body
                  %h3= features[:secret_network][:features][:yield_optimizer][:name]
                  %p.mb-0
                    %b Blockchain:
                    = cl_image_tag(features[:secret_network][:cloudinary_public_id], width: 18, height: 18, crop: :fill, dpr: 2, aspect_ratio: 1, fetch_format: :auto, quality: :auto)
                    = features[:secret_network][:name].to_s
                  %form#connect-wallet-form{ name: 'connectWalletForm' }
                    %button.btn.btn-md.btn-grad.w-100#connect-wallet-button{ type: 'submit' }
                      .d-none#connect-wallet-button-loading
                        %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                        %em Loading...
                      #connect-wallet-button-ready
                        Connect
          .row.mb-5
            .col-12
              .card.mb-3
                .card-body
                  .row
                    .col-sm-3
                      = cl_image_tag(logo_cloudinary_public_id, crop: :scale, height: 24, dpr: 2, fetch_format: :auto, quality: :auto, class: 'align-top mr-2 d-none d-sm-inline', style: 'height: 24px;')
                      BUTT
                      %br
                      Profit distributor
                    .col-sm-4
                      Wallet:
                      %span.butt-wallet-balance-loading Loading...
                      %span.d-none.butt-wallet-balance-link
                        %span.d-none.butt-wallet-balance
                        %button.btn.btn-light.btn-sm.ml-2.border.butt-wallet-balance-view-button{ type: 'button' }
                          .d-none
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          .ready
                            .fas.fa-search.mr-1
                            View
                      %br
                      Deposited:
                      %span.profit-distributor-user-shares Loading...
                      %br
                      Claimable SEFI:
                      %span.profit-distributor-claimable-profit Loading...
                    .col-sm-3
                      TVL:
                      %span.profit-distributor-total-shares Loading...
                    .col-sm-2
                      %h3= link_to '>', '#profit-distributor-collapse', 'data-toggle' => 'collapse'
                  .row.collapse#profit-distributor-collapse
                    .col-12
                      %hr
                    .col-sm-6
                      Deposit:
                      %span.butt-wallet-balance-loading Loading...
                      %span.d-none.butt-wallet-balance-link
                        %span.d-none.butt-wallet-balance
                        %button.btn.btn-light.btn-sm.ml-2.border.butt-wallet-balance-view-button{ type: 'button' }
                          .d-none
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          .ready
                            .fas.fa-search.mr-1
                            View
                      %form{ name: 'profitDistributorDepositForm' }
                        .form-group
                          %input.form-control{ name: 'amount' }
                        %button.btn.btn-primary.btn-rg#profit-distributor-deposit-button{ type: 'submit' }
                          .d-none#profit-distributor-deposit-button-loading
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          #profit-distributor-deposit-button-ready
                            Deposit
                    .col-sm-6
                      Withdraw:
                      %span.profit-distributor-user-shares Loading...
                      = link_to '#', class: 'd-none butt-profit-distributor-balance-link' do
                        %span.d-none.butt-wallet-balance
                        %button.btn.btn-light.btn-sm.ml-2.border.butt-wallet-balance-view-button{ type: 'button' }
                          .d-none
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          .ready
                            .fas.fa-search.mr-1
                            View
                      %form{ name: 'profitDistributorWithdrawForm' }
                        .form-group
                          %input.form-control{ name: 'amount' }
                        %button.btn.btn-primary.btn-rg#profit-distributor-withdraw-button{ type: 'submit' }
                          .d-none#profit-distributor-withdraw-button-loading
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          #profit-distributor-withdraw-button-ready
                            Withdraw
                    .col-12
                      %hr
                      Fees: nil
              // Yield optimizer B
              .card
                .card-body
                  .row
                    .col-sm-3
                      = cl_image_tag(logo_cloudinary_public_id, crop: :scale, height: 24, dpr: 2, fetch_format: :auto, quality: :auto, class: 'align-top mr-0 d-none d-sm-inline', style: 'height: 24px;')
                      = cl_image_tag('logos/wbtc_ethereum_iwfxwu', crop: :scale, height: 24, dpr: 2, fetch_format: :auto, quality: :auto, class: 'align-top mr-2 d-none d-sm-inline', style: 'height: 24px;')
                      BUTT-sWBTC LP
                      %br
                      Earn Buttcoin
                    .col-sm-3
                      Wallet:
                      %span.butt-swbtc-lp-wallet-balance-loading Loading ...
                      %span.d-none.butt-swbtc-lp-wallet-balance-link
                        %span.d-none.butt-swbtc-lp-wallet-balance
                        %button.btn.btn-light.btn-sm.ml-2.border.butt-swbtc-lp-wallet-balance-view-button{ type: 'button' }
                          .d-none
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          .ready
                            .fas.fa-search.mr-1
                            View
                      %br
                      Deposited:
                      %span.yield-optimizer-b-user-shares Loading...
                      %br
                      Claimable BUTT:
                      %span.yield-optimizer-b-claimable-butt Loading...
                    .col-sm-2
                      Buttcoin per block: 1.4
                      %br
                      Starting block: 5,060,000
                      %br
                      End block: 15,060,000
                    .col-sm-2
                      TVL: 
                      %span.yield-optimizer-b-total-shares Loading...
                    .col-sm-2
                      %h3= link_to '>', '#yield-optimizer-b-collapse', 'data-toggle' => 'collapse'
                  .row.collapse#yield-optimizer-b-collapse
                    .col-12
                      %hr
                    .col-sm-6
                      Balance:
                      %span.butt-swbtc-lp-wallet-balance-loading Loading ...
                      %span.d-none.butt-swbtc-lp-wallet-balance-link
                        %span.d-none.butt-swbtc-lp-wallet-balance
                        %button.btn.btn-light.btn-sm.ml-2.border.butt-swbtc-lp-wallet-balance-view-button{ type: 'button' }
                          .d-none.butt-swbtc-lp-wallet-balance-loading
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          .ready
                            .fas.fa-search.mr-1
                            View
                      %form{ name: 'yieldOptimizerBDepositForm' }
                        .form-group
                          %input.form-control{ name: 'amount' }
                        %button.btn.btn-primary.btn-rg#yield-optimizer-b-deposit-button{ type: 'submit' }
                          .d-none#yield-optimizer-b-deposit-button-loading
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          #yield-optimizer-b-deposit-button-ready
                            Deposit
                    .col-sm-6
                      Withdraw:
                      %span.yield-optimizer-b-user-shares Loading...
                      = link_to '#', class: 'd-none yield-optimizer-b-balance-link' do
                        %span.d-none.butt-wallet-balance
                        %button.btn.btn-light.btn-sm.ml-2.border.butt-wallet-balance-view-button{ type: 'button' }
                          .d-none
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          .ready
                            .fas.fa-search.mr-1
                            View
                      %form{ name: 'yieldOptimizerBWithdrawForm' }
                        .form-group
                          %input.form-control{ name: 'amount' }
                        %button.btn.btn-primary.btn-rg#yield-optimizer-b-withdraw-button{ type: 'submit' }
                          .d-none#yield-optimizer-b-withdraw-loading
                            %em.spinner-grow.spinner-grow-sm{ 'aria-hidden' => 'true', :role => 'status' }
                            %em Loading...
                          #yield-optimizer-b-withdraw-button-ready
                            Withdraw
                    .col-12
                      %hr
                      Fees: 0

