!!!
%html.js{ lang: 'eng' }
  %head
    %title btn.group
    %meta{ charset: 'utf-8' }
    %meta{ content: 'width=device-width, initial-scale=1, shrink-to-fit=no', name: 'viewport' }
    %meta{ content: 'We develop software for blockchain.', name: 'description' }
    %link{ href: 'https://res.cloudinary.com/hv5cxagki/image/upload/c_scale,dpr_2,f_auto,h_15,q_auto/v1/logos/btn_purple_background_uu2ere', rel: 'shortcut icon' }
    = csrf_meta_tags
    = csp_meta_tag

    -# === THEME STYLESHEETS ===
    = stylesheet_pack_tag 'application', media: 'all'
    = javascript_pack_tag 'application'
    %link{ href: '/ico-crypto-html-v1.9.3/main-html/assets/css/vendor.bundle.css?ver=1930', rel: 'stylesheet' }
    %link#changeTheme{ href: '/ico-crypto-html-v1.9.3/main-html/assets/css/style-salvia.css?ver=1930', rel: 'stylesheet' }
    %link{ href: '/ico-crypto-html-v1.9.3/main-html/assets/css/theme.css?ver=1930', rel: 'stylesheet' }
  %body.nk-body.body-wider.theme-dark.mode-onepage
    .nk-wrap
      %header.nk-header.page-header.is-transparent.is-shrink#header
        / Header @s
        .header-main
          .container
            .header-wrap
              / Logo @s
              .header-logo.logo
                = link_to root_path, class: 'logo-link' do
                  = cl_image_tag(logo_cloudinary_public_id, crop: :scale, height: 36, dpr: 2,  fetch_format: :auto, quality: :auto, class: 'align-top mr-2 d-none d-sm-inline', style: 'height: 36px;')
                  = cl_image_tag(logo_cloudinary_public_id, crop: :scale, height: 24, dpr: 2,  fetch_format: :auto, quality: :auto, class: 'align-top mr-2 d-inline d-sm-none', style: 'height: 24px;')
                  %h3.text-white.d-inline btn.group
              / Menu Toogle @s
              .header-nav-toggle
                %a.navbar-toggle{ 'data-menu-toggle' => 'header-menu', href: '#' }
                  .toggle-line
                    %span
              / Menu @s
              .header-navbar.header-navbar-s2
                %nav.header-menu.justify-content-between#header-menu
                  %ul.menu
                    %li.menu-item= link_to 'Features', features_path, class: 'menu-link nav-link'
                    %li.menu-item= link_to 'Buttcoin', secret_network_buttcoin_index_path, class: 'menu-link nav-link'
        = yield
        %footer.nk-footer.bg-theme.ov-h.overlay-x.fw-4
          %section.section.section-m.tc-light.tc-light-alt.bg-transparent.pb-0
            .container
              .nk-block.block-footer
                .row.my-md-0.mb-n4
                  .col-lg-9
                    .wgs.wgs-menu
                      %h6.wgs-title.text-white Company
                      .wgs-body
                        %ul.wgs-links.wgs-links-s3
                          %li= link_to 'Features', features_path
                          %li= link_to 'Buttcoin', secret_network_buttcoin_index_path
                          %li= link_to 'Team', "#{root_url}#team"
                  .col-lg-3.col-md
                    .wgs.wgs-subscribe-form
                      %h6.wgs-title.wgs-title-s2.text-white NewsLetter
                      .wgs-body
                        .wgs-subscribe#mc_embed_signup
                          %form.nk-form-submit.validate#mc-embedded-subscribe-form{ action: 'https://group.us6.list-manage.com/subscribe/post?u=eb5f49c9e796d26b7737589fb&id=3ba7db12dc', method: 'post', target: '_blank', name: 'mc-embedded-subscribe-form' }
                            .field-inline.field-inline-s3.field-split#mc_embed_signup_scroll
                              .field-wrap
                                %input.input-bordered.required.email#mce-EMAIL{ name: 'EMAIL', type: 'email', placeholder: 'Enter your email' }
                              #anti-spam-container{ 'aria-hidden' => 'true' }
                                %input{ name: 'b_eb5f49c9e796d26b7737589fb_3ba7db12dc', tabindex: '-1', type: 'text', value: '' }
                              .submit-wrap
                                %button.btn.btn-icon.btn-light.btn-auto
                                  %i.fas.fa-paper-plane
                            .clear#mce-responses
                              .response#mce-error-response
                              .response#mce-success-response
                        -# Mailchimp
                        %script{ src: '//s3.amazonaws.com/downloads.mailchimp.com/js/mc-validate.js', type: 'text/javascript' }
                        :javascript
                          (function($) {window.fnames = new Array(); window.ftypes = new Array();fnames[0]='EMAIL';ftypes[0]='email';fnames[1]='FNAME';ftypes[1]='text';fnames[2]='LNAME';ftypes[2]='text';fnames[3]='ADDRESS';ftypes[3]='address';fnames[4]='PHONE';ftypes[4]='phone';fnames[5]='BIRTHDAY';ftypes[5]='birthday';}(jQuery));var $mcj = jQuery.noConflict(true);

              .footer-bottom.section-sm
                .row.justify-content-between.align-items-center
                  .col-6
                    = link_to root_path, class: 'logo-link wgs-logo-s2' do
                      = cl_image_tag(logo_cloudinary_public_id, crop: :scale, height: 36, dpr: 2, fetch_format: :auto, quality: :auto, class: 'align-top mr-2 d-none d-sm-inline', style: 'height: 36px;')
                      = cl_image_tag(logo_cloudinary_public_id, crop: :scale, height: 24, dpr: 2, fetch_format: :auto, quality: :auto, class: 'align-top mr-2 d-inline d-sm-none', style: 'height: 24px;')
                      %h3.text-white.d-inline btn.group
                  / .col
                  .col-6
                    .copyright-text.text-right
                      %p Â© btn.group, 2021
    .preloader
      %span.spinner.spinner-round

/ JavaScript
%script{ src: '/ico-crypto-html-v1.9.3/main-html/assets/js/jquery.bundle.js?ver=1930' }
%script{ src: '/ico-crypto-html-v1.9.3/main-html/assets/js/scripts.js?ver=1930' }
%script{ src: '/ico-crypto-html-v1.9.3/main-html/assets/js/charts.js?var=161' }
